           BEGIN 
             MENUITEM "Mark sets in current group" , IDM_MARKSETSINCURCOMP , MFT_STRING 
             MENUITEM "Mark current set" , IDM_MARKAREAS, MFT_STRING
             MENUITEM "Un-mark current set", IDM_UNMARKAREAS, MFT_STRING
             MENUITEM "View marked sets", IDM_VIEWMARKS, MFT_STRING
             MENUITEM "Invert marks" , IDM_INVERTMARKS , MFT_STRING
             MENUITEM "Delete marked sets" , IDM_DELETEMARKS , MFT_STRING 
             MENUITEM "Delete un-marked sets" , IDM_DELETEUNMARKS , MFT_STRING 
             MENUITEM "Erase all marks", IDM_ERASEALLMARKS, MFT_STRING
             MENUITEM " " , 0 , MFT_SEPARATOR
             MENUITEM "Mark sets with species..." , IDM_JUDGESPECIESMRK , MFT_STRING
             MENUITEM " " , 0 , MFT_SEPARATOR
             MENUITEM "Show sets within current", IDM_INREFWITHIN, MFT_STRING
             MENUITEM "Show sets containing current", IDM_INREFCONTAINING, MFT_STRING
             MENUITEM "Show sets disjunct with current", IDM_INREFDISJUNCT, MFT_STRING
             MENUITEM "Show sets partly overlapping current", IDM_INREFPARTIAL, MFT_STRING
             MENUITEM "Show sets similar to current", IDM_SHOWSUPERSET , MFT_STRING
             MENUITEM " " , 0 , MFT_SEPARATOR
             MENUITEM "Show species distributions", IDM_SHOWALLSPECIES , MFT_STRING
             MENUITEM "Show scoring species", IDM_SHOWSCORINGSPECIES , MFT_STRING
             MENUITEM " " , 0 , MFT_SEPARATOR
             MENUITEM "Delete this set", IDM_DELETEVAR , MFT_STRING
             MENUITEM "Create a new (empty) set", IDM_CREATEANEWSET , MFT_STRING
             MENUITEM "Create a new set piling up marks", IDM_PILEUPMARKS , MFT_STRING
             MENUITEM "Create a new set from this species" , IDM_SETFROMSPECIES , MFT_STRING 
          END 
       
       MENUITEM "  <<  ", IDM_GOTOPREVSET , MFT_STRING
       MENUITEM "  >>  ", IDM_GOTONEXTSET , MFT_STRING


   END


