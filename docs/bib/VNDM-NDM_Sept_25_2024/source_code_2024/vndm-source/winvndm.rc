            MENUITEM "Pa&rsimony analysis (assumed=1)" , IDM_SECTSEARCH , MFT_STRING 
            MENUITEM " " , 0 , MFT_SEPARATOR
            MENUITEM "Optimize grid position" , IDM_CSSCYCLES , MFT_STRING 
            MENUITEM " " , 0 , MFT_SEPARATOR
            MENUITEM "&Select active taxa" , IDM_MARKIDTAXA , MFT_STRING 
            MENUITEM "Get list of taxa to inactivate from file" , IDM_GETTAXA_TO_WIPE , MFT_STRING 
            MENUITEM "Write list of endemic taxa to output file" , IDM_LISTTAX , MFT_STRING 
            MENUITEM " " , 0 , MFT_SEPARATOR
            MENUITEM "Consense areas..." , IDM_CONSENSUS , MFT_STRING
         END

       POPUP "&Settings", IDM_SETTINGS, MFT_STRING
         BEGIN 
            MENUITEM "Allow data/set edition", IDM_CHARDATAEDIT, MFT_STRING
            POPUP "Auto-edit", IDM_UNLOCKED, MFT_STRING
              BEGIN
                MENUITEM "Do not auto-edit" , IDM_SHOWNONE , MFT_STRING 
                MENUITEM " " , 0 , MFT_SEPARATOR
                MENUITEM "Make cells on/off" , IDM_SHOWADD , MFT_STRING 
                MENUITEM "Make cells assumed" , IDM_SORTTAXA , MFT_STRING 
                MENUITEM "Make cells defined/undefined" , IDM_SHOWACT , MFT_STRING 
              END
            MENUITEM "Query before overwrites/deletes" , IDM_BEEP , MFT_STRING 
            MENUITEM "Track mouse movements" , IDM_SELECTMOUSE , MFT_STRING 
            MENUITEM "Report score differences" , IDM_DESCRIBE , MFT_STRING 
            POPUP "Colors..." , IDM_COLORS , MFT_STRING 
              BEGIN 
               MENUITEM "Reset cell colors" , IDM_VARCOLORS , MFT_STRING 
               MENUITEM "Black and white" , IDM_BLACK , MFT_STRING 
              END
            MENUITEM "No queries" , IDM_MAKENOQUERIES , MFT_STRING 
            MENUITEM " " , 0 , MFT_SEPARATOR
            MENUITEM "Set radius size" , IDM_SWITCHMATRIX , MFT_STRING 
            MENUITEM "Select species to plot" , IDM_TAXSTATS , MFT_STRING  
            MENUITEM "Show individual records" , IDM_RESOLUTION , MFT_STRING 
            MENUITEM "Set grid size and position" , IDM_INCREASEFONTSIZE , MFT_STRING  
            POPUP "System of coordinates..." , IDM_COORDINATESYSTEM , MFT_STRING 
              BEGIN 
                 MENUITEM "Transpose X-Y" , IDM_TRANSPOSEXY , MFT_STRING 
                 MENUITEM "Flip (on X)" , IDM_FLIPONX , MFT_STRING 
                 MENUITEM "Flip (on Y)" , IDM_FLIPONY , MFT_STRING 
                 MENUITEM "Back to start..." , IDM_BACKTOSTART , MFT_STRING 
              END   
            MENUITEM "Show points only" , IDM_ONLY_POINTS , MFT_STRING 
            MENUITEM "Define narrow  territory" , IDM_EXPANDVAR , MFT_STRING 
            MENUITEM " " , 0 , MFT_SEPARATOR
            MENUITEM "New map line" , IDM_NEWMAPLINE , MFT_STRING 
            MENUITEM "Hide map beyond grid" , IDM_HIDEMAP, MFT_STRING 
            MENUITEM "Last point edition..." , IDM_LASTPOINT , MFT_STRING 
            MENUITEM "Lock grid size (edit map only)" , IDM_LOCKGRID , MFT_STRING 
          END 
          
        POPUP "&Help" , IDM_CONSOLE , MFT_STRING , 
           BEGIN 
             MENUITEM "&Help", IDM_HELP, MFT_STRING
             MENUITEM "&Copyright Notice", IDM_COPYRIGHT, MFT_STRING
             MENUITEM "A&bout", IDM_ABOUT, MFT_STRING
           END 

