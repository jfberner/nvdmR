              BEGIN
                MENUITEM "Endemic species" , IDM_WRITESPPINFO , MFT_STRING 
                MENUITEM "Sets (=areas)" , IDM_WRITESETSINFO , MFT_STRING 
                MENUITEM "Areas for chosen species..." , IDM_JUDGESPECIESTXT , MFT_STRING
                MENUITEM "Consensus areas" , IDM_WRITECONSINFO , MFT_STRING
                MENUITEM "Overlaps" , IDM_REPORTOVERLAPS , MFT_STRING 
