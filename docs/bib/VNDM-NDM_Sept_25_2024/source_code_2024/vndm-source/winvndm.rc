                BEGIN 
                MENUITEM "Save for GlobalMapper" , IDM_LOLLIPOP , MFT_STRING
                MENUITEM "Save for DIVAGIS" , IDM_DIVALOLLIPOP , MFT_STRING 
                END 
            MENUITEM " " , 0 , MFT_SEPARATOR
            MENUITEM "Create &matrix from data points" , IDM_NEWMATRIX , MFT_STRING
            MENUITEM " " , 0 , MFT_SEPARATOR
            MENUITEM "e&Xit", IDM_EXIT, MFT_STRING
          END 
       POPUP "&Analyze", IDM_GETANALYZE, MFT_STRING
         BEGIN 
            MENUITEM "&Analyze with NDM" , IDM_SEARCH , MFT_STRING 
            MENUITEM "Create script to run Linux NDM" , IDM_OUTDESCRIBE , MFT_STRING 
            MENUITEM "Set &factors" , IDM_DFTFACTOR , MFT_STRING 
            MENUITEM "Set minimum species score" , IDM_SETMINIMUMSPSCORE , MFT_STRING 
            MENUITEM "Use edge proportions" , IDM_SETEDGEUSAGE , MFT_STRING 
            MENUITEM "Weight species", IDM_WEIGHTSPECIES , MFT_STRING 
